// This is a simple raw storage DNA file made for testing. Storage files make
// up the base of libpandadna, and are very simple. Their job is to store
// models, textures, and fonts for later use in sz/town DNA files.

model "path/models/my_model_0" [
	store_node [ "my_root" "my_code_and_search_path" ]
	store_node [ "my_root" "my_code" "search_path" ]
]

place_model "path/models/my_model_1" [
	store_node [ "prop" "prop_my_model_1" "" ]
]

hood_model "path/models/my_model_2" [
	store_node [ "my_root" "hood_my_model_2" "" ]
]

store_font [ "font" "my_font_code"
	     "path/fonts/my_font_0" ]

store_texture [ "texture" "street_floor"
		"path/maps/floor.jpg" ]
store_texture [ "texture" "street_sidewalk"
		"path/maps/sidewalk.jpg" ]
store_texture [ "texture" "street_curb"
		"path/maps/curb.jpg" ]
